	public Statement atStaden(Statement td) {
	    String a = "insert into XS(XM, XB, CSSI, KCS, BZ, P) values (?, ?, ?, ?, ?, ?)";
	    try {
	        pst = conn.prepareStatement(a);
	        pst.setString(1, student.getXin());
	        pst.setString(2, student.getXb());
	        pst.setTimestamp(3, new Timestamp(student.getCss0()));
